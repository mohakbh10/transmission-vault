generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "postgresql"
}

model IntelligenceReport {
  id             String   @id @default(uuid())
  fileName       String
  status         String
  signalType     String
  decodedMessage String?
  confidence     Int?
  anomalies      String[]
  createdAt      DateTime @default(now())
}